* sh -evx default.o.do build/game.o build/game build/game.o.redo.tmp
SRC=${1#build/}
+ SRC=game.o
SRC=game/${SRC%%.o}
+ SRC=game/game
BUILDDIR=$(dirname $2)
+ dirname build/game
+ BUILDDIR=build

redo-ifchange $SRC.c
+ redo-ifchange game/game.c
mkdir -p $BUILDDIR
+ mkdir -p build
gcc -o $3 -c $SRC.c+ gcc -o build/game.o.redo.tmp -c game/game.c
